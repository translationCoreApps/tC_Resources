{
  "name": "tc_resources",
  "version": "0.7.1-beta.1",
  "description": "Updates bible resources for translationCore app",
  "main": "index.js",
  "scripts": {
    "test": "eslint ./helpers ./index.js && jest",
    "update-resource": "babel-node index.js",
    "update-en-resource": "babel-node index.js en",
    "prettier-resource": "prettier --single-quote --print-width 80 --trailing-comma all --write 'resources/**/*.json'"
  },
  "jest": {
    "collectCoverageFrom": [
      "helpers/**/*.js",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "coverage"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-eslint": "7.2.3",
    "jest": "21.2.1",
    "eslint": "4.7.2",
    "prettier": "1.7.0"
  },
  "dependencies": {
    "adm-zip": "0.4.7",
    "axios": "0.16.2",
    "babel-plugin-transform-async-to-generator": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "fs-extra": "3.0.1",
    "path-extra": "3.0.0",
    "usfm-js": "0.0.10",
    "yamljs": "0.3.0"
  }
}
