{
  "name": "tc_resources",
  "version": "0.7.1-beta.1",
  "description": "Updates bible resources for translationCore app",
  "main": "index.js",
  "scripts": {
    "test": "eslint ./helpers ./index.js && jest",
    "ugnt-parse": "babel-node --presets node6 ./scripts/ugntParse.js --ver=v0",
    "update-resource": "babel-node index.js",
    "update-ugnt-tw": "babel-node --presets node6 ./scripts/ugnt-tw.js --ver=v0 --lang=grc --bible=ugnt",
    "update-en-resource": "babel-node index.js en",
    "prettier-resource": "prettier --single-quote --print-width 80 --trailing-comma all --write 'resources/**/*.json'"
  },
  "jest": {
    "collectCoverageFrom": [
      "helpers/**/*.js",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "coverage"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "eslint": "^4.7.2",
    "jest": "^21.1.0",
    "prettier": "^1.5.3"
  },
  "dependencies": {
    "adm-zip": "^0.4.7",
    "axios": "^0.16.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-node6": "^11.0.0",
    "babel-register": "^6.11.6",
    "fs-extra": "^3.0.1",
    "path-extra": "3.0.0",
    "usfm-js": "1.0.0-beta.21",
    "yamljs": "^0.3.0"
  }
}
